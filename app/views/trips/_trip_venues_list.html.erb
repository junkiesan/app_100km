  <% trip.venues.each do |venue| %>
    <div class="trip-venue-card">
      <div class="venue-details">
        <h2><%= venue.name %></h2>
        <h6><%= venue.category %></h6>
      </div>
      <% if venue.photos.attached? %>
        <%= cl_image_tag venue.photos.first.key %>
      <% else %>
        <%= image_tag('venue-img.jpg') %>
      <% end %>
    </div>
  <% end %>
